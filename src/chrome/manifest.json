{
  "name": "JIRA 2 Excel",
  "version": "1.0",
  "description": "Build an Extension!",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://jira.cainc.com/secure/Dashboard.jspa"],
      "js": ["index.bundle.js"]
    }
  ], 
  "page_action": {
    "default_icon": {
      "16": "get_started16.png",
      "32": "get_started32.png",
      "48": "get_started48.png",
      "128": "get_started128.png"
    }
  },
  "icons": {
    "16": "get_started16.png",
    "32": "get_started32.png",
    "48": "get_started48.png",
    "128": "get_started128.png"
  },
  "permissions": ["activeTab", "declarativeContent", "storage"],
  "manifest_version": 2
}